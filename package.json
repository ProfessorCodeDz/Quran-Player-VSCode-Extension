{
  "name": "quran-player",
  "displayName": "Quran Player",
  "publisher": "RiadZ",
  "description": "Visual Studio Code extension that enables you to listen to the Quran with various reciters and includes all surahs.",
  "icon": "icon.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:quran-player.quranPlayer"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quran-player.quranPlayer",
        "title": "Quran Player"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  }
}
